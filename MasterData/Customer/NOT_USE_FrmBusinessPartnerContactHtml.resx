  <data name="tileView1.TileHtmlTemplate.Styles" xml:space="preserve">
    <value>.root {
	height: 100%;
	font-family: "Segoe UI";
	font-size: 14px;
	background-color: @Window;
	border-style: solid;
	border-width: 1px;
	border-color: @Border;
	border-radius: 8px;
	padding: 16px;
	box-shadow: 0 1px 3px @Black/0.1;
	position: relative;
}
.content {
	display: flex;
	flex-direction: row;
	align-items: flex-start;
	gap: 24px;
	height: 100%;
}
.avatar-container {
	flex-shrink: 0;
}
.photo {
	width: 56px;
	height: 56px;
	border-radius: 100px;
	border: solid 2px @Border;
	object-fit: cover;
	background-color: @DisabledControl/0.2;
}
.contact-info {
	flex-grow: 1;
	display: flex;
	flex-direction: column;
	gap: 10px;
	min-width: 0;
	width: 100%;
}
.name-row {
	display: flex;
	align-items: baseline;
	gap: 12px;
	flex-wrap: wrap;
	margin-bottom: 6px;
	width: 100%;
}
.name {
	font-weight: 600;
	color: @Primary;
	font-size: 15px;
	line-height: 1.3;
	white-space: nowrap;
}
.position {
	color: #757575;
	font-size: 13px;
	font-style: italic;
	white-space: nowrap;
	margin-left: 4px;
}
.primary-badge {
	background-color: #4CAF50;
	color: @White;
	padding: 3px 10px;
	border-radius: 12px;
	font-size: 11px;
	font-weight: 600;
	white-space: nowrap;
	margin-left: 8px;
}
.info-row {
	font-size: 13px;
	color: @WindowText;
	line-height: 1.5;
	margin-top: 2px;
	width: 100%;
	word-wrap: break-word;
	overflow-wrap: break-word;
}
.info-label {
	color: #757575;
	font-weight: 500;
	white-space: nowrap;
}
.info-value {
	font-weight: 500;
	color: @WindowText;
	word-wrap: break-word;
	overflow-wrap: break-word;
}
.status-active {
	color: #4CAF50;
	font-weight: 600;
}
.status-inactive {
	color: #F44336;
	font-weight: 600;
}
.contact-row {
	font-size: 12px;
	color: @DisabledText;
	line-height: 1.5;
	margin-top: 2px;
	width: 100%;
	word-wrap: break-word;
	overflow-wrap: break-word;
}
.indicator {
	position: absolute;
	top: 12px;
	right: 12px;
	width: 8px;
	height: 8px;
	border-radius: 50%;
	background-color: @Primary;
	box-shadow: 0 0 4px @Primary/0.5;
}
.indicator-inactive {
	background-color: #757575;
	box-shadow: none;
}
.checkbox-container {
	position: absolute;
	top: 12px;
	left: 12px;
	width: 24px;
	height: 24px;
	display: flex;
	align-items: center;
	justify-content: center;
	cursor: pointer;
	z-index: 10;
}
.checkbox {
	width: 20px;
	height: 20px;
	border: 2px solid @Border;
	border-radius: 4px;
	background-color: @Window;
	display: flex;
	align-items: center;
	justify-content: center;
	transition: all 0.2s;
	margin: 0;
	padding: 0;
}
.checkbox:hover {
	border-color: @Primary;
	background-color: @Primary/0.1;
}
.checkbox.checked {
	background-color: @Primary;
	border-color: @Primary;
}
.checkbox.checked::after {
	content: "✓";
	color: @White;
	font-size: 14px;
	font-weight: bold;
	line-height: 1;
	display: flex;
	align-items: center;
	justify-content: center;
}
.hidden {
	display: none;
}</value>
  </data>
  <data name="tileView1.TileHtmlTemplate.Template" xml:space="preserve">
    <value>&lt;div class="root"&gt;
	&lt;div class="indicator" dx-class="{IsActive:,indicator-inactive}"&gt;&lt;/div&gt;
	&lt;div class="checkbox-container" id="checkbox" onclick="OnCheckboxClick"&gt;
		&lt;div class="checkbox" dx-class="{Selected:checked}"&gt;&lt;/div&gt;
	&lt;/div&gt;
	&lt;div class="content"&gt;
		&lt;div class="avatar-container" dx-class="{AvatarBase64:,hidden}"&gt;
			&lt;img class="photo" src="${AvatarBase64}"/&gt;
		&lt;/div&gt;
		&lt;div class="contact-info"&gt;
			&lt;div class="name-row"&gt;
				&lt;span class="name"&gt;${FullName}&lt;/span&gt;
				&lt;span class="position" dx-class="{Position:,hidden}"&gt;${Position}&lt;/span&gt;
				&lt;span class="primary-badge" dx-class="{IsPrimary:,hidden}"&gt;Liên hệ chính&lt;/span&gt;
			&lt;/div&gt;
			&lt;div class="info-row" dx-class="{SiteName:,hidden}"&gt;
				&lt;span class="info-label"&gt;Chi nhánh:&lt;/span&gt;
				&lt;span class="info-value"&gt; ${SiteName}&lt;/span&gt;
			&lt;/div&gt;
			&lt;div class="info-row"&gt;
				&lt;span class="info-label"&gt;Trạng thái:&lt;/span&gt;
				&lt;span class="status-active" dx-class="{IsActive:,hidden}"&gt; Hoạt động&lt;/span&gt;
				&lt;span class="status-inactive" dx-class="{IsActive:hidden}"&gt; Không hoạt động&lt;/span&gt;
			&lt;/div&gt;
			&lt;div class="contact-row" dx-class="{Phone:,hidden}"&gt;
				&lt;span class="info-label"&gt;ĐT:&lt;/span&gt;
				&lt;span class="info-value"&gt; ${Phone}&lt;/span&gt;
			&lt;/div&gt;
			&lt;div class="contact-row" dx-class="{Email:,hidden}"&gt;
				&lt;span class="info-label"&gt;Email:&lt;/span&gt;
				&lt;span class="info-value"&gt; ${Email}&lt;/span&gt;
			&lt;/div&gt;
		&lt;/div&gt;
	&lt;/div&gt;
&lt;/div&gt;</value>
  </data>
